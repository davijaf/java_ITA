
public class Pilha {
	
	private Object elemento;
	private int quantidade;

	public boolean estaVazia() {
		return (elemento == null);
	}

	public int tamanho() {
		// TODO Auto-generated method stub
		return 0;
	}

	public void empilha(Object elemento) {
		this.elemento = elemento;
		quantidade++; 
		// TODO Auto-generated method stub
		
	}

	public Object topo() {
		// TODO Auto-generated method stub
		return elemento;
	}

}
